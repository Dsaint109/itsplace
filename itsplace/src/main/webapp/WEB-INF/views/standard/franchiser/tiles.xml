  			<add-attribute value="franchiser/1.0/fid.css" />
  			<add-attribute value="franchiser/1.0/franchiserDetail.css" />
  		</put-list-attribute>
	</definition>


	<definition name="franchiser/franchiserDetail" extends="base_layout">
		<put-attribute name="body"	 expression="/WEB-INF/views/${currentSitePreference.mobile ? 'mobile/' : 'standard/'}franchiser/franchiserDetail.jsp" />		

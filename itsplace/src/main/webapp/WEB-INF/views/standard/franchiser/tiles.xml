    		<add-attribute value="openapi/daummap.js" />        		       		
    		<add-attribute value="common/1.0/json-1.0.0.js" />      
    		<add-attribute value="jquery/cluetip/jquery.cluetip.js" />      
    		<add-attribute value="jquery/rightclick/jquery.rightClick.js" />      
    		<add-attribute value="jquery/jquery-ui-1.8.16.custom/js/jquery-ui-1.8.16.custom.min.js" />
    		    
  		</put-list-attribute>
  		<put-list-attribute name="styles">      		
  			<add-attribute value="franchiser/1.0/fid.css" />
  					<add-attribute value="jquery/jquery-ui-1.8.16.custom/css/ui-lightness/jquery-ui-1.8.16.custom.css" />

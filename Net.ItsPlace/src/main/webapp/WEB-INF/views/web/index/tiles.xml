<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 2.1//EN" "http://tiles.apache.org/dtds/tiles-config_2_1.dtd">

<tiles-definitions>

	<definition name="web/index/index" extends="empty_layout">		
		<put-attribute name="body" expression="/WEB-INF/views/web/index/index.jsp" />
    		
	  <put-list-attribute name="scripts">				
		      <add-attribute value="js/mosaic/js/mosaic.1.0.1.min.js" />
		      <add-attribute value="admin/components/tipsy/jquery.tipsy.js" />
		      <add-attribute value="admin/components/fancybox/jquery.fancybox.js" />
  		</put-list-attribute>  			
		
		<put-list-attribute name="styles">  		
  			<add-attribute value="common/common.css" />
  			<add-attribute value="font/font-awesome.css" />
  			<add-attribute value="common/normalize.css" />
  			<add-attribute value="js/mosaic/css/mosaic.css" />
  			<add-attribute value="admin/components/fancybox/jquery.fancybox.css" />
  		</put-list-attribute>
  		 
	</definition>
	
    
</tiles-definitions>
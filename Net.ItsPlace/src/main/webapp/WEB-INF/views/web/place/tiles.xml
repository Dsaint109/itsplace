<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 2.1//EN" "http://tiles.apache.org/dtds/tiles-config_2_1.dtd">

<tiles-definitions>

	<definition name="web/place/list" extends="empty_layout">		
		<put-attribute name="body" expression="/WEB-INF/views/web/place/list.jsp" />		
		<put-list-attribute name="scripts">				
		      <add-attribute value="js/nodejs/socket.io.js" />
		      <add-attribute value="js/tiles/placeon.js" />
		       <add-attribute value="js/tiles/Tile.js" />
		      <add-attribute value="js/tiles/demo.js" />
  		</put-list-attribute>  			
		
		<put-list-attribute name="styles">
  			<add-attribute value="common/normalize.css" />
  			<add-attribute value="css/tiles.css" />
  			
  		</put-list-attribute>
	</definition>
	<definition name="web/place/test" extends="empty_layout">		
		<put-attribute name="body" expression="/WEB-INF/views/web/place/test.jsp" />		
		<put-list-attribute name="scripts">				
	
		      <add-attribute value="js/nodejs/socket.io.js" />
  		</put-list-attribute>  			
		
		<put-list-attribute name="styles">
  			<add-attribute value="common/normalize.css" />
  			<add-attribute value="css/tiles.css" />
  			
  		</put-list-attribute>
	</definition>
	
	<definition name="web/place/view" extends="empty_layout">		
		<put-attribute name="body" expression="/WEB-INF/views/web/place/view.jsp" />		
		
		<put-list-attribute name="scripts">				
		       <add-attribute value="js/slider/jquery.ui.touch-punch.min.js" />
		       <add-attribute value="js/slider/bannerscollection_kenburns.js" />
  		</put-list-attribute>  			
		
		<put-list-attribute name="styles">
  			<add-attribute value="common/normalize.css" />  		
  			<add-attribute value="css/place.css" />
  			<add-attribute value="js/slider/bannerscollection_kenburns.css" />
  			
  		</put-list-attribute>
	</definition>
	
    
</tiles-definitions>
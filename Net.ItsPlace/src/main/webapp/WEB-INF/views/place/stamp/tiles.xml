<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 2.1//EN" "http://tiles.apache.org/dtds/tiles-config_2_1.dtd">

<tiles-definitions>

	<definition name="place/**" extends="place_layout">
		<put-attribute name="body"	 expression="/WEB-INF/views/place/{1}.jsp" />		
		<put-list-attribute name="scripts">
  		</put-list-attribute>  		
  		<put-list-attribute name="styles">  			
  			<add-attribute value="common/stamp.css" />
  		</put-list-attribute>		
	</definition>     
	<definition name="place/stamp/burn" extends="admin_empty_layout">
		<put-attribute name="body"	 expression="/WEB-INF/views/place/stamp/burn.jsp" />		
		<put-list-attribute name="styles">  			
  			<add-attribute value="common/stamp.css" />
  		</put-list-attribute>			
	</definition>     
	
</tiles-definitions>
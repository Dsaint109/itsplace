<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMapConfig
	PUBLIC "-//ibatis.apache.org//DTD SQL Map Config 2.0//EN"
	"http://ibatis.apache.org/dtd/sql-map-config-2.dtd">

<sqlMapConfig>
	<settings useStatementNamespaces="true"/>

	<!-- 사용자로그인 -->
	<sqlMap resource="net/itsplace/user/user-sqlmap.xml" />
	

	
	<!-- UTIL  -->
	<sqlMap resource="net/itsplace/util/util-sqlmap.xml" />
	<sqlMap resource="net/itsplace/common/common-sqlmap.xml" />
	
	<!-- 웹페이지 관련 SQL -->	
	<sqlMap resource="net/itsplace/web/sql/feature-sqlmap.xml" />
	<sqlMap resource="net/itsplace/web/sql/index-sqlmap.xml" />
	<sqlMap resource="net/itsplace/web/sql/search-sqlmap.xml" />
	<sqlMap resource="net/itsplace/web/sql/stamp-sqlmap.xml" />
	<sqlMap resource="net/itsplace/web/sql/support-sqlmap.xml" />
	<sqlMap resource="net/itsplace/web/sql/place-sqlmap.xml" />
	<!--// 웹페이지 관련 SQL -->
	
	<!-- 관리자 관련 SQL -->
	<sqlMap resource="net/itsplace/admin/sql/stamp-sqlmap.xml" />
	<sqlMap resource="net/itsplace/admin/sql/support-sqlmap.xml" />
	<sqlMap resource="net/itsplace/admin/sql/media-sqlmap.xml" />

	<!--// 가맹점 관련 SQL -->
	<sqlMap resource="net/itsplace/place/sql/place-user-sqlmap.xml" />
	<sqlMap resource="net/itsplace/place/sql/place-stamp-sqlmap.xml" />
	<sqlMap resource="net/itsplace/place/sql/place-info-sqlmap.xml" />
	<sqlMap resource="net/itsplace/place/sql/place-comment-sqlmap.xml" />
	<sqlMap resource="net/itsplace/place/sql/place-menu-sqlmap.xml" />
	<sqlMap resource="net/itsplace/place/sql/place-review-sqlmap.xml" />
	
</sqlMapConfig>